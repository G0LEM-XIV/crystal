{
    "name": "dinoqqq/crystal",
    "description": "Queue worker to easily manage recurring tasks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daan",
            "email": "daan@kuijstenwebcompany.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "monolog/monolog": "^2.2",
        "catfan/medoo": "^1.7",
        "psr/container": "^1."
    },
    "autoload": {
        "psr-4": {
            "Crystal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crystal\\Test\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "6.4.4",
        "phake/phake": "3.*"
    }
}
